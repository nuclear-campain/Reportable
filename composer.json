{
    "name": "b61/laravel-reportable",
    "description": "reportable Polymorphic Eloquent Models for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-reportable", "Laravel Reportable", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@brianfaust.me",
            "homepage": "https://brianfaust.me"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6",
        "illuminate/database": "^5.6"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "ActivismBE\\Reportable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActivismBE\\Tests\\Reportable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ActivismBE\\Reportable\\ReportableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
